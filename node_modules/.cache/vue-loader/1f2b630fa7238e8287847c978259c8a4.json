         <v-card>\n                    <v-img\n                        :src=\"posterImage(item)\"\n                        aspect-ratio=\"1\"\n                        :alt=\"item.title\"\n                    ></v-img>\n\n                    <v-card-title primary-title>\n                        <div>\n                            <h2>{{ item.title }}</h2>\n                            <div>Release Date: {{ item.release_date }}</div>\n                            <div>Type: Movie</div>\n                            <div>Ratings: {{ item.vote_average }}</div>\n                        </div>\n                    </v-card-title>\n\n                    <v-card-actions class=\"justify-center\">\n                        <v-btn text color=\"green\" @click=\"singleMovie(item.id)\"\n                            >View</v-btn\n                        >\n                    </v-card-actions>\n                </v-card>\n            </v-flex>\n        </v-layout>\n    </v-container>\n</template>\n\n<script>\nexport default {\n    data() {\n        return {\n            gottaWatchMovies: [],\n            empty: false,\n            loading: true\n        };\n    },\n\n    mounted: function() {\n        // if we have a list in local storage, replace the sample data\n        if (localStorage.getItem('gottaWatchMovies')) {\n            this.gottaWatchMovies = JSON.parse(\n                localStorage.getItem('gottaWatchMovies')\n            );\n            this.loading = false;\n        } else {\n            // eslint-disable-next-line no-console\n            console.log('No local storage');\n            this.loading = false;\n        }\n\n        if (this.gottaWatchMovies.length === 0) {\n            this.empty = true;\n        }\n    },\n\n    computed: {},\n\n    methods: {\n        singleMovie(id) {\n            this.$router.push('/movie/' + id);\n        },\n\n        posterImage(item) {\n            let baseImgUrl = 'https://image.tmdb.org/t/p/';\n            return baseImgUrl.concat('w154', item.poster_path);\n        }\n    }\n};\n</script>\n"]}]}